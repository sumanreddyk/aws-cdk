{
  "compilerOptions": {
    "target":"ES2018",
    "module": "commonjs",
    "lib": ["es2016", "es2017.object", "es2017.string"],
    "declaration": true,
    "strict": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization":false,
    "outDir": "./dist/",
    "rootDir": "./src/",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "aws-lambda",
      "lodash"
    ],
  },
  "exclude": [
    "code",
    "dist"
  ]
}
